<body>
    <div class="row wrapper_div">
        <div class="col-12 cvcomp">

            
                <div class="cv_container">
                    <div class="heading">
                        <div class="name">
                                    <span id="firstName">
                                            {{#current_resume.personal.firstName}}
                                                {{current_resume.personal.firstName}}
                                            {{/current_resume.personal.firstName}}
                                            {{^current_resume.personal.firstName}}
                                                Name
                                            {{/current_resume.personal.firstName}}
                                            </span> <span id="lastName">
                                            {{#current_resume.personal.lastName}}
                                                {{current_resume.personal.lastName}}
                                            {{/current_resume.personal.lastName}}
                                            {{^current_resume.personal.lastName}}
                                                Name
                                            {{/current_resume.personal.lastName}}
                                            </span>
                                </div>
                                <span id="position" class="position">
                                            {{#current_resume.personal.position}}
                                                {{current_resume.personal.position}}
                                            {{/current_resume.personal.position}}
                                        </span>
                                <div class="contact_info">
                                    <span class="address">
                                            <span class="address_icon">{{#current_resume.personal.address}}<i class="fa-solid fa-location-dot"></i>{{/current_resume.personal.address}}</span>
                                            <div class="address_city_wrapper">
                                                <span class="address_text">
                                                    {{#current_resume.personal.address}}
                                                        {{current_resume.personal.address}}
                                                    {{/current_resume.personal.address}}
                                                </span>
                                                <span class="city_country_text">
                                                    <span class="city text">{{#current_resume.personal.city}}{{current_resume.personal.city}}{{/current_resume.personal.city}}</span><span class="comma {{^current_resume.personal.country}}d-none{{/current_resume.personal.country}}">,</span>
                                                    <span class="country text">{{#current_resume.personal.country}}{{current_resume.personal.country}}{{/current_resume.personal.country}}</span>
                                                </span>
                                            </div>
                                    </span>
                                    <span class="phone">
                                            <span class="phone_icon">{{#current_resume.personal.phone}}<i class="fa-solid fa-phone"></i>{{/current_resume.personal.phone}}</span>
                                            <span class="right_text">
                                                {{#current_resume.personal.phone}}
                                                    {{current_resume.personal.phone}}
                                                {{/current_resume.personal.phone}}
                                            </span>
                                    </span>
                                    <span class="email">
                                            <span class="email_icon">{{#current_resume.personal.email}}<i class="fa-solid fa-envelope"></i>{{/current_resume.personal.email}}</span>
                                            <span class="right_text">
                                                {{#current_resume.personal.email}}
                                                    {{current_resume.personal.email}}
                                                {{/current_resume.personal.email}}
                                            </span>
                                    </span>
                                </div>
                    </div>

                    <div class="main_content">
                        <div class="row">
                                    
                                    <div class="col-8 left_main_content">
                                        {{#profile}}
                                            <div class="profile">
                                                <h3 class="profile_title">Profile</h3>
                                                <div class="desc">
                                                    {{#current_resume.profile.profile_desc}}
                                                        {{current_resume.profile.profile_desc}}
                                                    {{/current_resume.profile.profile_desc}}
                                                </div>
                                            </div>
                                        {{/profile}}
                                        {{#experience}}
                                            <div class="experience">
                                                <h3 class="experience__title">Experience</h3>
                                                <div class="experience__list">
                                                    {{#current_resume.experience}}
                                                            <div class="experience__list__item experience__list__item__{{index}}">
                                                                <span class="position">{{position}}</span>
                                                                <div class="company_wrapper editor">
                                                                    <div class="experience_address_wrapper">
                                                                        <span class="company">{{company}}</span>
                                                                        <span class="company_address">{{#companyaddress}}{{companyaddress}}{{/companyaddress}}</span>
                                                                    </div>
                                                                    <span class="dates">
                                                                        <span class="date_span startMonth">{{startMonth}}</span>
                                                                        <span class="date_span startYear">{{startYear}}</span>
                                                                        <span class="date_span delimeter"> - </span>
                                                                        {{#endMonth}}
                                                                            <span class="date_span endMonth">{{endMonth}}</span>
                                                                            <span class="date_span endYear">{{endYear}}</span>
                                                                        {{/endMonth}}
                                                                        {{^endMonth}}
                                                                            <span class="date_span present">Present</span>
                                                                        {{/endMonth}}
                                                                    </span>
                                                                </div>
                                                                {{#textarea_type_div}}
                                                                    <div class="description">
                                                                        {{textarea_prof_desc}}
                                                                    </div>
                                                                {{/textarea_type_div}}
                                                                {{^textarea_type_div}}
                                                                    <ul class="description ul_holder">
                                                                        {{{textarea_prof_desc}}}
                                                                    </ul>
                                                                {{/textarea_type_div}}
                                                            </div>
                                                        {{/current_resume.experience}}                
                                                </div>
                                            </div>
                                        {{/experience}}
                                        {{#education}}
                                            <div class="education">
                                                <h3 class="education__title">Education</h3>
                                                <div class="education__list">
                                                    {{#current_resume.education}}
                                                        <div class="education__list__item education__list__item__{{index}}">
                                                            <span class="degree">{{degree}}</span>
                                                            <div class="company_wrapper editor">
                                                                <div class="education_address_wrapper">
                                                                    <span class="school">{{school}}</span>
                                                                    <span class="school_address">{{#schooladdress}}{{schooladdress}}{{/schooladdress}}</span>
                                                                </div>
                                                                <span class="dates">
                                                                    <span class="date_span startMonth">{{startMonth}}</span>
                                                                    <span class="date_span startYear">{{startYear}}</span>
                                                                    <span class="date_span delimeter"> - </span>
                                                                    {{#endMonth}}
                                                                        <span class="date_span endMonth">{{endMonth}}</span>
                                                                        <span class="date_span endYear">{{endYear}}</span>
                                                                    {{/endMonth}}
                                                                    {{^endMonth}}
                                                                        <span class="date_span">Present</span>
                                                                    {{/endMonth}}
                                                                </span>
                                                            </div>
                                                            {{#textarea_type_div}}
                                                                <div class="description">
                                                                    {{textarea_prof_desc}}
                                                                </div>
                                                            {{/textarea_type_div}}
                                                            {{^textarea_type_div}}
                                                                <ul class="description ul_holder">
                                                                    {{{textarea_prof_desc}}}
                                                                </ul>
                                                            {{/textarea_type_div}}
                                                        </div>
                                                    {{/current_resume.education}}
                                                </div>
                                            </div>
                                        {{/education}}
                                        </div>

                                        <div class="col-4 right_main_content">
                                            {{#skills}}
                                                <div class="skills">
                                                    <h2>Skills</h2>
                                                    {{#current_resume.skills}}
                                                        <div class="skill_rating{{index}}">
                                                            <span class="skill_name">{{skill}}</span>
                                                            <span class="rating star_{{starValue}}">
                                                                <span class="star1"></span>
                                                                <span class="star2"></span>
                                                                <span class="star3"></span>
                                                                <span class="star4"></span>
                                                                <span class="star5"></span>
                                                            </span>
                                                        </div>
                                                    {{/current_resume.skills}}
                                                </div>
                                            {{/skills}}
                                            {{#languages}}
                                                <div class="languages">
                                                    <h2>Languages</h2>
                                                    {{#current_resume.languages}}
                                                        <div class="language_div_{{index}}">
                                                            <div class="language">{{language}}</div>
                                                            <div class="range_wrapper">
                                                                <div class="language_range" style="width: {{rangeValue}}%;"></div>
                                                            </div>
                                                        </div>
                                                    {{/current_resume.languages}}
                                                </div>
                                            {{/languages}}
                                            {{#certifications}}
                                                <div class="certifications">
                                                    <h2 class="certification__title">Certifications</h2>
                                                    <div class="certifications__list">
                                                        {{#current_resume.certifications}}
                                                            <div class="certifications__list__item__{{index}}">
                                                                {{certification}}
                                                            </div>
                                                        {{/current_resume.certifications}}
                                                    </div>
                                                </div>
                                            {{/certifications}}
                                        
                                    </div>

                                </div>
                    </div>

                </div>
        </div>
    </div>          
</body>

</html>
<body>
    
    <div class="row wrapper_div">
        <div class="col-12 cvcomp">
                        <div class="cv1">

                                <div class="row cv_container">
                                    <div class="leftCol col-4">
                                        <div class="image_cont">
                                            <img class="prof_image" id="cvProfilePhoto" src="{{imageProfile}}" alt="profile picture"/>
                                        </div>
                                        <div class="personal">
                                            <h2>Personal</h2>
                                        </div>

                                        <div class="bio">
                                            <div class="bio_group">
                                                <span class="weighted" data-text="Address">
                                                    {{#current_resume.personal.address}}
                                                        Address
                                                    {{/current_resume.personal.address}}
                                                </span>
                                                <span class="address" id="address">{{current_resume.personal.address}}</span>
                                                <span class="city" id="city">{{current_resume.personal.city}}</span>
                                            </div>
                                            <div class="bio_group">
                                                <span class="weighted" data-text="Phone">
                                                    {{#current_resume.personal.phone}}
                                                        Phone
                                                    {{/current_resume.personal.phone}}
                                                </span>
                                                <span class="phone" id="phone">{{current_resume.personal.phone}}</span>
                                            </div>
                                            <div class="bio_group">
                                                <span class="weighted" data-text="Email">
                                                    {{#current_resume.personal.email}}
                                                        Email
                                                    {{/current_resume.personal.email}}
                                                </span>
                                                <span id="email">{{current_resume.personal.email}}</span>
                                            </div>
                                        </div>
                                    {{#skills}}
                                        <div class="skills">
                                            <h2>Skills</h2>
                                            {{#current_resume.skills}}
                                                <div class="skill_div_{{index}}">
                                                    <div class="skill">{{skill}}</div>
                                                    <div class="range_wrapper">
                                                        <div class="skill_range" style="width: {{rangeValue}}%"></div>
                                                    </div>
                                                </div>
                                            {{/current_resume.skills}}
                                        </div>
                                    {{/skills}}
                                    {{#languages}}
                                        <div class="languages">
                                            <h2>Languages</h2>
                                            
                                            {{#current_resume.languages}}
                                                <div class="language_div_{{index}}">
                                                    <span class="language_name">{{language}}</span>
                                                    <span class="rating star_{{starValue}}">
                                                        <span class="star1"></span>
                                                        <span class="star2"></span>
                                                        <span class="star3"></span>
                                                        <span class="star4"></span>
                                                        <span class="star5"></span>
                                                    </span>
                                                </div>
                                            {{/current_resume.languages}}

                                        </div>
                                    {{/languages}}
                                    </div>
                                    <div class="rightCol col-8">
                                        <div class="name">
                                            <h1><span id="firstName">
                                                {{#current_resume.personal.firstName}}
                                                    {{current_resume.personal.firstName}}
                                                {{/current_resume.personal.firstName}}
                                                {{^current_resume.personal.firstName}}
                                                    Name
                                                {{/current_resume.personal.firstName}}
                                            </span> <span id="lastName">
                                                {{#current_resume.personal.lastName}}
                                                    {{current_resume.personal.lastName}}
                                                {{/current_resume.personal.lastName}}
                                                {{^current_resume.personal.lastName}}
                                                    Name
                                                {{/current_resume.personal.lastName}}
                                            </span></h1>
                                        </div>
                                    {{#profile}}
                                        <div class="desc">
                                            {{#current_resume.profile.profile_desc}}
                                                {{current_resume.profile.profile_desc}}
                                            {{/current_resume.profile.profile_desc}}
                                        </div>
                                    {{/profile}}
                                    {{#experience}}
                                        <div class="experience">
                                            <h3 class="experience__title">Work Experience</h3>
                                            <div class="experience__list">
                                                {{#current_resume.experience}}
                                                    <div class="experience__list__item experience__list__item__{{index}}">
                                                        <span class="position">{{position}}</span>
                                                        <div class="company_wrapper editor">
                                                            <div class="experience_address_wrapper">
                                                                <span class="company">{{company}}</span>
                                                                <span class="company_address">{{#companyaddress}}{{companyaddress}}{{/companyaddress}}</span>
                                                            </div>
                                                            <span class="dates">
                                                                <span class="date_span startMonth">{{startMonth}}</span>
                                                                <span class="date_span startYear">{{startYear}}</span>
                                                                <span class="date_span delimeter"> - </span>
                                                                {{#endMonth}}
                                                                    <span class="date_span endMonth">{{endMonth}}</span>
                                                                    <span class="date_span endYear">{{endYear}}</span>
                                                                {{/endMonth}}
                                                                {{^endMonth}}
                                                                    <span class="date_span">Present</span>
                                                                {{/endMonth}}
                                                            </span>
                                                        </div>
                                                        {{#textarea_type_div}}
                                                            <div class="description">
                                                                {{textarea_prof_desc}}
                                                            </div>
                                                        {{/textarea_type_div}}
                                                        {{^textarea_type_div}}
                                                            <ul class="description ul_holder">
                                                                {{{textarea_prof_desc}}}
                                                            </ul>
                                                        {{/textarea_type_div}}
                                                    </div>
                                                {{/current_resume.experience}}
                                            </div>
                                        </div>
                                    {{/experience}}
                                    {{#education}}
                                        <div class="education">
                                            <h3 class="education__title">Education</h3>
                                            <div class="education__list">
                                                {{#current_resume.education}}
                                                    <div class="education__list__item education__list__item__{{index}}">
                                                        <span class="position">{{degree}}</span>
                                                        <div class="company_wrapper editor">
                                                            <div class="education_address_wrapper">
                                                                <span class="school">{{school}}</span>
                                                                <span class="school_address">{{#schooladdress}}{{schooladdress}}{{/schooladdress}}</span>
                                                            </div>
                                                            <span class="dates">
                                                                <span class="date_span startMonth">{{startMonth}}</span>
                                                                <span class="date_span startYear">{{startYear}}</span>
                                                                <span class="date_span delimeter"> - </span>
                                                                {{#endMonth}}
                                                                    <span class="date_span endMonth">{{endMonth}}</span>
                                                                    <span class="date_span endYear">{{endYear}}</span>
                                                                {{/endMonth}}
                                                                {{^endMonth}}
                                                                    <span class="date_span">Present</span>
                                                                {{/endMonth}}
                                                            </span>
                                                        </div>
                                                        {{#textarea_type_div}}
                                                            <div class="description">
                                                                {{textarea_prof_desc}}
                                                            </div>
                                                        {{/textarea_type_div}}
                                                        {{^textarea_type_div}}
                                                            <ul class="description ul_holder">
                                                                {{{textarea_prof_desc}}}
                                                            </ul>
                                                        {{/textarea_type_div}}
                                                    </div>
                                                {{/current_resume.education}}
                                            </div>
                                        </div>
                                    {{/education}}
                                    </div>
                                </div>
                        </div>
                </div>
        </div>
</body> 
</html>